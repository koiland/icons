{
  "name": "@koiland/icons",
  "version": "1.0.0",
  "description": "SVG icons",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run prebuild:icons && rollup -c",
    "clean": "rimraf src/icons && rimraf ./dist",
    "prebuild:icons": "npm run svgo && npm run svgr",
    "svgo": "svgo --config ./svgo.config.js -f ./src/static/svg -o ./src/static/svg",
    "svgr": "svgr --typescript --title-prop --no-dimensions -d src/icons -- src/static/svg",
    "storybook": "npm run clean && npm run prebuild:icons && start-storybook -p 6006",
    "build-storybook": "npm run clean && npm run prebuild:icons && build-storybook -o docs-build",
    "lint": "eslint src/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/koiland/icons.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "icons",
    "svg",
    "react"
  ],
  "author": "dev@koiland.io",
  "license": "ISC",
  "sideEffects": false,
  "homepage": "https://github.com/koiland/icons",
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "bugs": {
    "url": "https://github.com/koiland/icons/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "FORCE_COLOR=1 lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-webpack4": "^6.5.3",
    "@storybook/manager-webpack4": "^6.5.3",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "0.0.11",
    "@svgr/cli": "^6.2.1",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^4.3.8",
    "lint-staged": "^8.0.4",
    "prettier": "^2.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-filesize": "^9.1.1",
    "styled-components": "^5.3.5",
    "svgo": "^2.8.0",
    "typescript": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">= 18.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3"
  }
}
